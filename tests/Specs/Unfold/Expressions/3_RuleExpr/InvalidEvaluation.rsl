scheme InvalidEvaluation =
    class
        type
            Range = {| i : Int :- i >= 0 /\ i < 4 |}
        
        transition_system [TS]
            variable
                x[t : Range]: Int,
                z[t : Range]: Int

            init_constraint
                all y : Range :- x[.y.] = 1,
                all y : Range :- z[.y.] = 1

            transition_rules
                z[.x[.1.].] = 1 ==> x'[.1.] = 1
        end
    end
